package taeho0920;

import java.util.Scanner;

public class Taeho0920 {

	public static void main(String[] args) {
		//구구단 만들기
		Scanner sc = new Scanner(System.in);//정수형을 입력 받기
		
		System.out.println("몇단이 보고싶니?");
		int 몇단 = sc.nextInt();

		int 곱 = 1;

		}	
		포문을 써서 구구단을 풀력
		for(int 포곱	=1; 포곱<=9;포곱++) {
			System.out.println(몇단+"*"+포곱+"="+(몇단*포곱));
		}
}	
